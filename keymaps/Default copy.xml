<keymap version="1" name="Default copy" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="Arrangement.Alias.Rule.Edit" />
  <action id="Arrangement.Rule.Edit" />
  <action id="Editor Redo">
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="EditorDeleteLine" />
  <action id="Git.Branches">
    <keyboard-shortcut first-keystroke="shift ctrl alt b" />
  </action>
  <action id="Git.CreateNewBranch">
    <keyboard-shortcut first-keystroke="shift ctrl alt k" />
  </action>
  <action id="Git.Fetch">
    <keyboard-shortcut first-keystroke="shift alt k" />
  </action>
  <action id="Git.Merge">
    <keyboard-shortcut first-keystroke="alt k" />
  </action>
  <action id="Git.Pull">
    <keyboard-shortcut first-keystroke="ctrl alt k" />
  </action>
  <action id="Git.Reword.Commit">
    <keyboard-shortcut first-keystroke="shift f6" />
  </action>
  <action id="GotoDeclaration">
    <keyboard-shortcut first-keystroke="ctrl b" />
