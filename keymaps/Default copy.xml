    <keyboard-shortcut first-keystroke="shift f6" />
    <keyboard-shortcut first-keystroke="f2" />
  </action>
  <action id="SavaAs" />
  <action id="SaveAll">
    <keyboard-shortcut first-keystroke="ctrl s" />
    <keyboard-shortcut first-keystroke="shift ctrl s" />
  </action>
  <action id="SelectInProjectView">
    <keyboard-shortcut first-keystroke="shift ctrl alt a" />
  </action>
  <action id="ShelvedChanges.Rename">
    <keyboard-shortcut first-keystroke="shift f6" />
  </action>
  <action id="XDebugger.EditWatch" />
  <action id="XDebugger.SetValue" />
